package xadrez;

public class XadrezException extends RuntimeException {

	public XadrezException() {
		// TODO Auto-generated constructor stub
	}

	public XadrezException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public XadrezException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	public XadrezException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public XadrezException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

}
